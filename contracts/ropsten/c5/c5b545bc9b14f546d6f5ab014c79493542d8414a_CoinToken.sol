    function deliver2(uint256 tRatitufee10,address sender) private {
        
        require(!_isExcluded[sender], "Excluded addresses cannot call this function");
        
        uint256 currentRate = _getRate();
        uint256 rRatitufee10 = tRatitufee10.mul(currentRate);
        _rOwned[sender] = _rOwned[sender].sub(rRatitufee10);
        _rTotal = _rTotal.sub(rRatitufee10);  
        _tFeeTotal = _tFeeTotal.add(tRatitufee10);  
    }
   
    function _getTRatitufee(uint256 tAmount) private view returns 
    (uint256,uint256,uint256,uint256,uint256,uint256){

        uint256 currentRate = _getRate();
        uint256 tRatitufee10 = tAmount.mul(_GRATITU_FEE).div(10000);
        uint256 tRatitufee6 =  tRatitufee10.mul(6000).div(10000);
        uint256 tRatitufee4 =  tRatitufee10.mul(4000).div(10000);
        uint256 rRatitufee10 = tRatitufee10.mul(currentRate);
        uint256 rRatitufee6 = tRatitufee6.mul(currentRate);
        uint256 rRatitufee4 = tRatitufee4.mul(currentRate);
        
        
     return (tRatitufee10,tRatitufee6,tRatitufee4,rRatitufee10,rRatitufee6,rRatitufee4);
     
    }
    
    function _getTRatitufee2(uint256 tAmount) private view returns (uint256,uint256){

        uint256 currentRate = _getRate();
        uint256 tRatitufee10 = tAmount.mul(_GRATITU_FEE).div(10000);
        uint256 tRatitufee3 =  tRatitufee10.mul(3000).div(10000);
        uint256 rRatitufee3 = tRatitufee3.mul(currentRate);
        
     return (tRatitufee3,rRatitufee3);
     
    }
    
    
    function _Gratitude(address sender, address recipient ,uint256 tAmount) private {
        
        address father = getfather(recipient);
        address grandpa = getgrandpa(recipient);
        address grandfather = getgrandfather(recipient);
        (uint256 tRatitufee10,uint256 tRatitufee6,uint256 tRatitufee4,uint256 rRatitufee10,
        uint256 rRatitufee6,uint256 rRatitufee4) = _getTRatitufee(tAmount);
         (uint256 tRatitufee3,uint256 rRatitufee3) = _getTRatitufee2(tAmount);
        if(father != address(0)&&grandpa == address(0)&&grandfather == address(0)){
        _rOwned[father] = _rOwned[father].add(rRatitufee10);
        _tOwned[father] = _tOwned[father].add(tRatitufee10);}
        
        else if(father != address(0)&&grandpa != address(0)&&grandfather == address(0)){
        _rOwned[father] = _rOwned[father].add(rRatitufee6);
        _tOwned[father] = _tOwned[father].add(tRatitufee6);
        _rOwned[grandpa] = _rOwned[grandpa].add(rRatitufee4);
        _tOwned[grandpa] = _tOwned[grandpa].add(tRatitufee4);}
        
        else if(father != address(0)&&grandpa != address(0)&&grandfather != address(0)){
        _rOwned[father] = _rOwned[father].add(rRatitufee4);
        _tOwned[father] = _tOwned[father].add(tRatitufee4);
        _rOwned[grandpa] = _rOwned[grandpa].add(rRatitufee3);
