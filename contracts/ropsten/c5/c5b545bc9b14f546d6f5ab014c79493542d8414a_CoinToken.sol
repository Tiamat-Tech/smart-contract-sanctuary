    }
    
    function TradingLimit(address sender,  uint256 amount) private {
        uint32 userPoint = checkPoint[sender] + Duration;  
        require(uint32(block.timestamp) >= userPoint );
        require(amount <= MaxValue);
        if(amount != invitePoint*_DECIMALFACTOR)
         {checkPoint[sender] = uint32(block.timestamp);}
    }
    
    function setDuration(uint32 _time,uint256 _Value ) public {
        Duration = _time;MaxValue = _Value.mul(_DECIMALFACTOR) ;}
