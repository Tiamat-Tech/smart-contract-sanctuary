////import "@openzeppelin/contracts/access/Ownable.sol";
////import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract DBLTools is Ownable {

    ///@dev batch airdrop tokens,the owner must have enough airdroped tokens.
    ///@param _users account addresses for recive token
    ///@param _amounts account need recive value
    ///@param _tokenaddr ERC20 Contract address 
    function batchTransfer(address[] memory _users,uint256[] memory _amounts, address _tokenaddr) onlyOwner external{
        require(_tokenaddr != address(0) && isContract(_tokenaddr) , " DBLBOY: TOKEN ADDRESS ERROR");
        require(_users.length > 0 && _amounts.length > 0 && _users.length == _amounts.length, "DBLBOY: INPUT ARRAY ERROR");
        for(uint256 i=0; i<_users.length; i++){
            require(_users[i] != address(0) && _amounts[i] != 0);
            IERC20(_tokenaddr).transfer(_users[i], _amounts[i]);
        }
    }

    ///@dev Return the exceeds tokens
    ///@param _tokenaddr ERC20 Contract address 
    function withdrawalToken(address _tokenaddr) onlyOwner external { 
        require(_tokenaddr != address(0) && isContract(_tokenaddr) , " DBLBOY: TOKEN ADDRESS ERROR");
        IERC20(_tokenaddr).transfer(owner(), IERC20(_tokenaddr).balanceOf(address(this)));
    }

    /**
     * @dev Returns true if `account` is a contract.
     *
     * [////IMPORTANT]
     * ====
     * It is unsafe to assume that an address for which this function returns
     * false is an externally-owned account (EOA) and not a contract.
     *
     * Among others, `isContract` will return false for the following
     * types of addresses:
     *
     *  - an externally-owned account
     *  - a contract in construction
     *  - an address where a contract will be created
     *  - an address where a contract lived, but was destroyed
     * ====
     */
    function isContract(address account) internal view returns (bool) {
        // This method relies on extcodesize, which returns 0 for contracts in
        // construction, since the code is only stored at the end of the
        // constructor execution.

        uint256 size;
        // solhint-disable-next-line no-inline-assembly
        assembly { size := extcodesize(account) }
        return size > 0;
    }
}