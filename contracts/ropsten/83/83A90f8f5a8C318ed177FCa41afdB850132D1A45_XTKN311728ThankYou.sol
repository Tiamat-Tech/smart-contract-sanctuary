/*
    This is the thank you contract from the presale to reward all presale holders with additional tokens!
    - Will send out tokens to all presale participants
*/
contract XTKN311728ThankYou is Context, Ownable{
    // lib
        using SafeMath for uint256; // more safe & secure uint256 operations
        using Address for address; // more safe & secure address operations
        using Array256 for uint256[]; // added functionality to arrays

    // address
        address public ADDRESS_BURN = 0x000000000000000000000000000000000000dEaD; // burn baby, burn!
        address public ADDRESS_THE_HORDE; // project contract to store value
        address public ADDRESS_TOKEN; // address of the main token
        address public ADDRESS_PRESALE_VAULT;// locked tokens after presale
        address public ADDRESS_PRESALE_VAULT20; // locked tokens for additional presale rewards

    // instance
        ERC20 private _token; // interface for parent token
        IXTKN311728PreSaleVault private _vault; // interface for investor vault
        IXTKN311728PreSaleVault20 private _vault20; // interface for additional investor vault

    // investors
        address[] public INVESTORS;
        mapping(address => uint256) public INVESTMENTS;
        uint256 private _indexThankYou;
        uint256 private _indexSend;
        uint256 private _indexWallets;
        uint256 public THANK_YOU = 25011 * (10**9);
        uint256 public WALLETS;

        uint256 private _10k =     10000 * (10**9);
        uint256 private _25k =     25000 * (10**9);
        uint256 private _50k =     50000 * (10**9);
        uint256 private _100k =   100000 * (10**9);
        uint256 private _250k =   250000 * (10**9);
        uint256 private _500k =   500000 * (10**9);
        uint256 private _1000k = 1000000 * (10**9);
        uint256[] private _pots = [_1000k, _500k, _250k, _100k, _50k, _25k, _10k];
        uint256[] private _multiplicator = [4, 4, 4, 2, 2, 2, 2];

        bool public THANK_YOU_COMPLETE = false;
        bool public SEND_COMPLETE = false;
        bool public WALLET_COMPLETE = false;

        

    // events
        event ContractCreation(); // emit wenn token is created
        event ContractDestroy(address indexed locker, uint256 time); // emit when contract is destroyed
        event TokenSet(address token, uint256 balance); // emit when the main contract token is configured
        event ThankYouVeryMuch(address indexed wallet, uint256 amount);
        event WalletsComplete(uint256 wallets, uint256 index);
        event sentVault20(address indexed wallet, uint256 tokens);

    // contract can be paid
    receive() external payable {}

    constructor(address pToken){
        if(ADDRESS_TOKEN == address(0) && pToken != address(0)){
            // setup main token instance
            ADDRESS_TOKEN = pToken;
            _token = ERC20(ADDRESS_TOKEN);
            emit TokenSet(ADDRESS_TOKEN, _token.balanceOf(address(this)));
        }
        emit ContractCreation();
    }



    // owner
    // ███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗
    // ╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝
    
    function init(address pPresaleVault, address pPresaleVault20, address pTheHorde) public onlyOwner{
        if(ADDRESS_PRESALE_VAULT == address(0)){
            ADDRESS_PRESALE_VAULT = pPresaleVault;
            ADDRESS_PRESALE_VAULT20 = pPresaleVault20;
            ADDRESS_THE_HORDE = pTheHorde;

            _vault = IXTKN311728PreSaleVault(ADDRESS_PRESALE_VAULT);
            _vault20 = IXTKN311728PreSaleVault20(ADDRESS_PRESALE_VAULT20);

            (, WALLETS, , , ) = _vault.stats();
        }
    }



    // public
    // ███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗
    // ╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝

    function kill() public{
        require(THANK_YOU_COMPLETE, "[xtkn311728][thank you] thank you process not complete yet!");
        require(SEND_COMPLETE, "[xtkn311728][thank yout] vault 2.0 process not complete yet!");
        _destroy(); // kill yourself
    }

    function wallets(uint256 pMax) public returns(bool){
        return(_wallets(pMax));
    }

    function send(uint256 pMax) public returns(bool){
        require(WALLET_COMPLETE, "[xtkn311728][thank you] not all wallets have been indexed, wait!");

        uint256 j;
        uint256 reward;

        for(uint256 i=_indexSend; i<INVESTORS.length; i++){
            if(j <= pMax){
                reward = _reward(INVESTMENTS[INVESTORS[i]]);
                require(_token.transfer(INVESTORS[i], reward),"[xtkn311728][thank you] token transfer failed!");
                _vault20.deposit(INVESTORS[i], reward);

                /* increase indexes */
                j = j.add(1);
                _indexSend = _indexSend.add(1);

                // event
                    emit sentVault20(INVESTORS[i], reward);
            }else{
                break; // exit loop
            }
        }

        if(_indexSend >= INVESTORS.length){
            SEND_COMPLETE = true;
            return(true);
        }
        return(false);
    }

    function thankYou(uint256 pMax) public returns(bool){
        require(WALLET_COMPLETE, "[xtkn311728][thank you] not all wallets have been indexed, wait!");

        uint256 j;

        for(uint256 i=_indexThankYou; i<INVESTORS.length; i++){
            if(j <= pMax){
                require(_token.transfer(INVESTORS[i], THANK_YOU),"[xtkn311728][thank you] token transfer failed!");

                /* increase indexes */
                j = j.add(1);
                _indexThankYou = _indexThankYou.add(1);

                // event
                    emit ThankYouVeryMuch(INVESTORS[i], THANK_YOU);
            }else{
                break; // exit loop
            }
        }

        if(_indexThankYou >= INVESTORS.length){
            THANK_YOU_COMPLETE = true;
            return(true);
        }
        return(false);
    }




    // private
    // ███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗███████╗
    // ╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝╚══════╝
  
    function _reward(uint256 pTokens) private view returns(uint256){
        uint256 reward;

        for(uint256 i=0; i<_pots.length; i++){
            if(pTokens >= _pots[i]){
                reward = reward.add(_pots[i].mul(_multiplicator[i]));
                pTokens = pTokens.sub(_pots[i]);
            }
        }
        
        return(reward);
    }

    function _wallets(uint256 pMax) private returns(bool){
        address wallet; // store current wallet address in loop
        uint256 reward; // store current rewards in looop

        uint256 j;
        uint256 gasWallet;

        for(uint256 i=_indexWallets; i<WALLETS; i++){
            gasWallet = gasleft();
            if(j <= pMax || gasWallet > 1000){
                wallet = _vault.LOCKED_ADDRESSES(i); // get investor wallet address
                INVESTORS.push(wallet); // save investor address again
                (, reward, ) = _vault.info(wallet); // get the reward from the presale contract for this wallet
                INVESTMENTS[wallet] = reward; // store info

                /* increase indexes */
                j = j.add(1);
                _indexWallets = _indexWallets.add(1);
            }else{
                break; // exit loop
            }
        }

        if(_indexWallets >= WALLETS){
            WALLET_COMPLETE = true;

            // event
                emit WalletsComplete(WALLETS, _indexWallets);

            return(true);
        }
        return(false);
    }    

    function _destroy() private{
        emit ContractDestroy(_msgSender(), block.timestamp);
        uint256 balance = _token.balanceOf(address(this));
        if(balance > 0){
            require(_token.transfer(ADDRESS_THE_HORDE, balance), "[xtkn311728][thank you] token transfer failed!"); // send all remaining tokens to the horde contract for future us
        }else{
            address payable destroy = payable(ADDRESS_THE_HORDE);
            selfdestruct(destroy);
        }
    }
}