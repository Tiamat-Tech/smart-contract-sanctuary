pragma solidity >=0.7.0 <0.9.0;

contract Class_1 {
    // 숫자형 선언
    uint a = 4;
    uint b = 2;
    
    //function name(input) character returns(output)
    function add(uint c, uint d) public view returns(uint) {
        return c+d;
    }
    
    function power(uint e) public view returns(uint) {
        return a**e;
    }
}