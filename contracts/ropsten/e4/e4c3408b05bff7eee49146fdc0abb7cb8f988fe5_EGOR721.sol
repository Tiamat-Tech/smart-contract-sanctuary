  // switch between set for sale and set not for sale
  function toggleForBNBSale(uint256 _tokenId) public {
    // require caller of the function is not an empty address
    require(msg.sender != address(0));
    // require that token should exist
    require(_exists(_tokenId));
    // get the token's owner
    address tokenOwner = ownerOf(_tokenId);
    // check that token's owner should be equal to the caller of the function
    require(tokenOwner == msg.sender);
    // get that token from all crypto boys mapping and create a memory of it defined as (struct => EgorNFT)
    EgorNFT memory egorNFT = allNFTs[_tokenId];
    // if token's forBNBSale is false make it true and vice versa
    if(egorNFT.forBNBSale) {
      egorNFT.forBNBSale = false;
    } else {
      egorNFT.forBNBSale = true;
    }
    // set and update that token in the mapping
    allNFTs[_tokenId] = egorNFT;
  }
  // switch between set for sale and set not for sale
  function toggleForRGOSale(uint256 _tokenId) public {
    // require caller of the function is not an empty address
    require(msg.sender != address(0));
    // require that token should exist
    require(_exists(_tokenId));
    // get the token's owner
    address tokenOwner = ownerOf(_tokenId);
