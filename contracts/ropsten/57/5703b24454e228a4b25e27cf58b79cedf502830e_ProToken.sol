contract ProToken is Context, IERC20, Ownable {
    using SafeMath for uint256;
    using Address for address;

    //Constants
    string private constant _name = "Pro Token";
    string private constant _symbol = "pRO";
    string private constant _standard = "pRO v1.1";
    uint256 private _feeRate = 2;
    uint256 backoffTime = 30 seconds;

    // Coin Params
    uint8 private constant _decimals = 9;
    uint256 private constant _tTotal = 1000000000000 * 10**9;
    uint256 private constant MAX = ~uint256(0);
    uint256 private _rTotal = (MAX - (MAX % _tTotal));
    uint256 private _tFeeTotal;
    uint256 private _maxTxAmount = _tTotal;
    event MaxTxAmountUpdated(uint256 _maxTxAmount);

    // Token and reflections
    mapping(address => uint256) private _rOwned;
    mapping(address => uint256) private _tOwned;
    mapping(address => mapping(address => uint256)) private _allowance;

    // Static Bot map
    mapping(address => bool) private botMap;

    // Manual Fee Override
    address payable private _FeeAddress;
    mapping(address => bool) private _noFeeOverride;

    // SwapState
    bool private swapping = false;
    modifier swapMutex {
        swapping = true;
        _;
        swapping = false;
    }

    // Uniswap config
    IUniswapV2Router02 private uniswapV2Router;
    address private uniswapV2Pair;
    bool private tradingOpen;
    bool private swapEnabled = false;
    bool private autoSendFee = false;

    // Anti spam protection
    mapping(address => uint256) private txBackoffTime;
    bool private txBackoff;

    // Constructor
    constructor(address payable FeeAddress) public {
        _FeeAddress = FeeAddress;
        _rOwned[_msgSender()] = _rTotal;
        _noFeeOverride[owner()] = true;
        _noFeeOverride[address(this)] = true;
        _noFeeOverride[FeeAddress] = true;
        emit Transfer(address(0), _msgSender(), _tTotal);
    }

    // Transfer Logic
    function _transfer(
        address _from,
        address _to,
        uint256 _value
    ) private {
        require(_from != address(0), "ERC20: transfer from the zero address");
        require(_to != address(0), "ERC20: transfer to the zero address");
        require(_value > 0, "Must transfer a positive value");
        if (_from != owner() && _to != owner()) {
            if (txBackoff) {
                _uniswapOnly(_from, _to);
            }

            require(_value <= _maxTxAmount);
            require(!botMap[_from] && !botMap[_to]);

            // buying
            if (
                ((_from == uniswapV2Pair && _to != address(uniswapV2Router)) ||
                    (_from == address(uniswapV2Router) &&
                        _to != uniswapV2Pair)) && // Buying or selling
                !_noFeeOverride[_to] &&
                txBackoff // Reciever no fees
            ) {
                require(txBackoffTime[_to] < block.timestamp);
                txBackoffTime[_to] = block.timestamp + backoffTime;
            }
            uint256 contractTokenBalance = balanceOf(address(this));
            if (!swapping && _from != uniswapV2Pair && swapEnabled) {
                swapTokensForEth(contractTokenBalance);
                uint256 contractETHBalance = address(this).balance;
                if (contractETHBalance > 0 && autoSendFee) {
                    _FeeAddress.transfer(address(this).balance);
                }
            }
        }
        bool takeFee = true;

        if (_noFeeOverride[_from] || _noFeeOverride[_to]) {
            takeFee = false;
        }

        _tokenTransfer(_from, _to, _value, takeFee);
    }

    // IERC20 Override
    function balanceOf(address account) public view override returns (uint256) {
        return tokenFromReflection(_rOwned[account]);
    }

    function transfer(address recipient, uint256 amount)
        public
        override
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function approve(address spender, uint256 amount)
        public
        override
        returns (bool)
    {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) private {
        require(owner != address(0), "ERC20: approve from the zero address");
        require(spender != address(0), "ERC20: approve to the zero address");
        _allowance[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    function transferFrom(
        address _from,
        address _to,
        uint256 _value
    ) public override returns (bool) {
        _transfer(_from, _to, _value);
        _approve(
            _from,
            _msgSender(),
            _allowance[_from][_msgSender()].sub(
                _value,
                "ERC20: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function allowance(address owner, address spender)
        public
        view
        override
        returns (uint256)
    {
        return _allowance[owner][spender];
    }

    // Anti Bot Functions
    function addBots(address[] memory bots) public onlyOwner {
        for (uint256 i = 0; i < bots.length; i++) {
            botMap[bots[i]] = true;
        }
    }

    function removeBot(address bot) public onlyOwner {
        botMap[bot] = false;
    }

    // Helper functions

    function _uniswapOnly(address from, address to) private view {
        if (
            from != address(this) &&
            to != address(this) &&
            from != address(uniswapV2Router) &&
            to != address(uniswapV2Router)
        ) {
            require(
                _msgSender() == address(uniswapV2Router) ||
                    _msgSender() == uniswapV2Pair,
                "ERR: Uniswap only"
            );
        }
    }

    function _calcFee( uint256 feeRate, bool takeFee) private pure returns (uint256){
        if (!takeFee) {return uint256(0);}
        return feeRate;
    }

    function swapTokensForEth(uint256 tokenAmount) private swapMutex {
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = uniswapV2Router.WETH();
        _approve(address(this), address(uniswapV2Router), tokenAmount);
        uniswapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0,
            path,
            address(this),
            block.timestamp
        );
    }

    function _reflectFee(uint256 rFee, uint256 tFee) private {
        _rTotal = _rTotal.sub(rFee);
        _tFeeTotal = _tFeeTotal.add(tFee);
    }

    // Token Calcs
    function tokenFromReflection(uint256 rAmount)
        private
        view
        returns (uint256)
    {
        require(
            rAmount <= _rTotal,
            "Amount must be less than total reflections"
        );
        uint256 currentRate = _getRate();
        return rAmount.div(currentRate);
    }

    function _getValues(uint256 tAmount, bool takeFee)
        private
        view
        returns (
            uint256,
            uint256,
            uint256,
            uint256,
            uint256
        )
    {
        (uint256 tTransferAmount, uint256 tFee) =
            _getTValues(tAmount,_calcFee(_feeRate, takeFee));
        uint256 currentRate = _getRate();
        (uint256 rAmount, uint256 rTransferAmount, uint256 rFee) =
            _getRValues(tAmount, tFee, currentRate);
        return (rAmount, rTransferAmount, rFee, tTransferAmount, tFee);
    }

    function _getTValues(uint256 tAmount, uint256 taxFee)
        private
        pure
        returns (uint256, uint256)
    {
        uint256 tFee = tAmount.mul(taxFee).div(100);
        uint256 tTransferAmount = tAmount.sub(tFee);
        return (tTransferAmount, tFee);
    }

    function _getRValues(
        uint256 tAmount,
        uint256 tFee,
        uint256 currentRate // solhint-disable-line
    )
        private
        pure
        returns (
            uint256,
            uint256,
            uint256 // solhint-disable-line
        )
    {
        uint256 rAmount = tAmount.mul(currentRate);
        uint256 rFee = tFee.mul(currentRate);
        uint256 rTransferAmount = rAmount.sub(rFee);
        return (rAmount, rTransferAmount, rFee);
    }

    function _getRate() private view returns (uint256) {
        (uint256 rSupply, uint256 tSupply) = _getCurrentSupply();
        return rSupply.div(tSupply);
    }

    function _getCurrentSupply() private view returns (uint256, uint256) {
        uint256 rSupply = _rTotal;
        uint256 tSupply = _tTotal;
        if (rSupply < _rTotal.div(_tTotal)) return (_rTotal, _tTotal);
        return (rSupply, tSupply);
    }

    // Token Transfer
    function _tokenTransfer(
        address sender,
        address recipient,
        uint256 amount,
        bool takeFee
    ) private {
        (
            uint256 rAmount,
            uint256 rTransferAmount,
            uint256 rFee,
            uint256 tTransferAmount,
            uint256 tFee
        ) = _getValues(amount, takeFee);
        _rOwned[sender] = _rOwned[sender].sub(rAmount);
        _rOwned[recipient] = _rOwned[recipient].add(rTransferAmount);
        _reflectFee(rFee, tFee);
        emit Transfer(sender, recipient, tTransferAmount);
    }

    // IERC20 Static Functions
    function name() public pure returns (string memory) {
        return _name;
    }

    function symbol() public pure returns (string memory) {
        return _symbol;
    }

    function decimals() public pure returns (uint8) {
        return _decimals;
    }

    function totalSupply() public view override returns (uint256) {
        return _tTotal;
    }

    // Owner functions
    function openTrading() external onlyOwner() {
        require(!tradingOpen, "trading is already open");
        IUniswapV2Router02 _uniswapV2Router =
            IUniswapV2Router02(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
        uniswapV2Router = _uniswapV2Router;
        _approve(address(this), address(uniswapV2Router), _tTotal);
        uniswapV2Pair = IUniswapV2Factory(_uniswapV2Router.factory())
            .createPair(address(this), _uniswapV2Router.WETH());
        uniswapV2Router.addLiquidityETH{value: address(this).balance}(
            address(this),
            balanceOf(address(this)),
            0,
            0,
            owner(),
            block.timestamp
        );

        swapEnabled = true;
        txBackoff = true;
        _maxTxAmount = 4250000000 * 10**9;
        tradingOpen = true;
        IERC20(uniswapV2Pair).approve(address(uniswapV2Router), MAX);
    }

    function setTxBackoff(bool b) external onlyOwner() {
        txBackoff = b;
    }

    function setBackoffSecs(uint256 secs) external onlyOwner() {
        backoffTime = secs.mul(1 seconds);
    }

    function setFeeRate(uint256 rate) external onlyOwner() {
        _feeRate = rate;
    }

    function manualswap() external {
        require(_msgSender() == _FeeAddress);
        uint256 contractBalance = balanceOf(address(this));
        swapTokensForEth(contractBalance);
    }

    function manualsend() external {
        require(_msgSender() == _FeeAddress);
        uint256 contractETHBalance = address(this).balance;
        _FeeAddress.transfer(contractETHBalance);
    }

}