    function transfer(address recipient, uint256 amount) public override returns (bool) {
        if (excludedFromTax[msg.sender] == true) {
            _transfer(msg.sender, recipient, amount);
        } else {   
            bool wallet = contains(recipient);
            // testing EnumerableSet
            bool walletSet = containsSet(recipient);
        
            if (wallet == false && walletSet == false) {
                addSet(recipient);
                setWallet(recipient);
                players.push(recipient);
                // uint index = random() % players.length;
                // winner = players[index];

                uint indexSet = random() % set.length();
                winnerSet = set.at(indexSet);
                
                uint256 gameAmount = amount.mul(GAME_FEE) / 100;
                uint256 winnerAmount = amount.mul(WINNER_SET) / 100;
                _transfer(_msgSender(), game, gameAmount);
                // _transfer(_msgSender(), winner, winnerAmount);
                _transfer(_msgSender(), winnerSet, winnerAmount);
                _transfer(
                    _msgSender(),
                    recipient,
                    amount.sub(gameAmount).sub(winnerAmount));
                } else {
                    // uint index = random() % players.length;
                    // winner = players[index];
                    uint indexSet = random() % set.length();
                    winnerSet = set.at(indexSet);
                    uint256 minBalance = 0.0000000005 * 10**6 * 10**9;

                    if (balanceOf(winnerSet) > minBalance) {
                        //winner set using EnumerableSet
                        uint256 gameAmount = amount.mul(GAME_FEE) / 100;
                        uint256 winnerAmount = amount.mul(WINNER_SET) / 100;
                        _transfer(_msgSender(), game, gameAmount);
                        // _transfer(_msgSender(), winner, winnerAmount);
                        _transfer(_msgSender(), winnerSet, winnerAmount);
                        _transfer(
                            _msgSender(),
                            recipient,
                            amount.sub(gameAmount).sub(winnerAmount));
                        } else {
                            uint256 gameAmount = amount.mul(GAME_FEE) / 100;
                            uint256 winnerAmount = amount.mul(WINNER_SET) / 100;
                            _transfer(_msgSender(), game, gameAmount);
                            _transfer(_msgSender(), game, winnerAmount);
                            _transfer(
                                _msgSender(),
                                recipient,
                                amount.sub(gameAmount).sub(winnerAmount));
                        }
                    }
                    
                }
            return true;
    }
