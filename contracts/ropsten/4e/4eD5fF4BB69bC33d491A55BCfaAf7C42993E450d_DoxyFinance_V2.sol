        );

        pancakePair = IFactory(_pancakeRouter.factory()).createPair(
            address(this),
            _pancakeRouter.WETH()
        );

        setPairContract(pancakePair);

        pancakeRouter = _pancakeRouter;

        stakePeriod = 180 days;
    }

    function UserLock(address Account, bool mode) public onlyOwner {
        LockList[Account] = mode;
    }

    function LockTokens(address Account, uint256 amount) public onlyOwner {
        LockedTokens[Account] = amount;
    }

    function UnLockTokens(address Account) public onlyOwner {
        LockedTokens[Account] = 0;
    }

    function mint(uint256 amount) external onlyOwner {
        _mint(msg.sender, amount);
    }

    function setPairContract(address _PairContract) public onlyOwner {
        PairContract = _PairContract;
    }

    function setStakePeriod(uint256 _days) public onlyOwner {
        stakePeriod = _days;
    }

    function setBuyLimit(uint256 buyLimit) external onlyOwner {
        _buyLimit = buyLimit;
    }

    function setSellLimit(uint256 sellLimit) external onlyOwner {
        _sellLimit = sellLimit;
    }

    function stake(
        address _address,
        string memory key,
        uint256 _value
    ) external onlyOwner returns (uint256) {
        uint256 time = block.timestamp;
        stakedBalances[_address][key] = _value;
        stakedTimestamp[_address][key] = time;
        return time;
    }

    function setBurnPercent(uint256 _burnPercent) external onlyOwner {
        require(
            _burnPercent >= 0 && _burnPercent < 100,
            "_burnPercent between 0 and 100"
        );
        burnPercent = _burnPercent;
    }
}