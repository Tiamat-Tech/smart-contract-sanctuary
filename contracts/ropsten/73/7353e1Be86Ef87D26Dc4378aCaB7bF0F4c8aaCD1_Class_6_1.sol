pragma solidity 0.8.0;

contract Class_6_1 {
    
        uint i;
    
    function add(uint i) public view returns (uint) {
        
        return i*i;

    }
    
    function add2(uint i) public view returns (uint) {
        
        return i*2;

    }
    
    function add3(uint i) public view returns (uint) {
        
        return i%3;

    }
    
}