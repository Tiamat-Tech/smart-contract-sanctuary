        symbol = "INZHAGI";
        proxyRegistryAddress[0x207Fa8Df3a17D96Ca7EA4f2893fcdCb78a304101] = true;
    }

    function contractURI() public pure returns (string memory) {
        return
            "https://us-central1-artis-life-network.cloudfunctions.net/contractURI2";
    }

    function setURI(string calldata new_uri) public onlyOwner {
        _setURI(new_uri);
        tokenURI = new_uri;
        uri = new_uri;
    }

    /*
        balanceOfEdition

        Preferred balanceOf function. Returns the balance of total NFTs 
        owned by the provided account address. 
        Edition numbers are preserved.
    */
    function balanceOfEdition(
        address account,
        uint256 id,
        uint256 edition
    ) public view returns (bool) {
        return nftData[id].editionOwnerships[edition] == account;
    }

    /*
        balanceOfEditionBatch

        Preferred balanceOfBatch function. Returns a batch of the balances
        of total NFTs owned by the provide account addresses. 
        Edition numbers are preserved.
    */
    function balanceOfEditionBatch(
        address[] calldata accounts,
        uint256[] calldata ids,
        uint256[] calldata editions
    ) public view returns (bool[] memory) {
        require(
            ids.length == accounts.length,
            "ERC1155: ids and amounts length mismatch"
        );
        bool[] memory batchBalances = new bool[](accounts.length);
        for (uint256 i = 0; i < ids.length; i++) {
            batchBalances[i] = this.balanceOfEdition(
                accounts[i],
                ids[i],
                editions[i]
            );
        }
        return batchBalances;
    }

    /*
        balanceOf

        Returns the balance of total NFTs owned by the provided
        account address. Edition numbers are not returned.
    */
    function balanceOf(address account, uint256 id)
        public
        view
        override
        returns (uint256)
    {
        return nftData[id].balanceOf[account];
    }

    /*
        balanceOfBatch

        Returns a batch of the balances of total NFTs owned by the provided
        account addresses. Edition numbers are not returned.
    */
    function balanceOfBatch(address[] calldata accounts, uint256[] calldata ids)
        public
        view
        override
        returns (uint256[] memory)
    {
        require(
            ids.length == accounts.length,
            "ERC1155: ids and amounts length mismatch"
        );
        uint256[] memory batchBalances = new uint256[](accounts.length);
        for (uint256 i = 0; i < ids.length; i++) {
            batchBalances[i] = this.balanceOf(accounts[i], ids[i]);
        }
        return batchBalances;
    }

    /*
        highestEdition

        Looks for the highest edition number of NFT id owned
        by the sender's address.
    */
    function highestEdition(address sender, uint256 id)
        private
        view
        returns (uint256)
    {
        for (uint256 i = nftData[id].TOTAL_SUPPLY - 1; i >= 0; i--) {
            if (nftData[id].editionOwnerships[i] == sender) return i;
        }
        revert("No editions");
    }

    /*
        createNewNFT

        Creates an NFT entry without minting any NFTs.
        Establishes name, symbol, totalSupply and creator.
    */
    function createNewNFT(
        string calldata nftName,
        string calldata nftSymbol,
        uint256 totalNFTSupply,
        address payable creator,
        uint256 royalty
    ) public onlyOwner {
        nftData[totalSupply].name = nftName;
        nftData[totalSupply].symbol = nftSymbol;
        nftData[totalSupply].TOTAL_SUPPLY = totalNFTSupply;
        nftData[totalSupply].creator = creator;
        creators[totalSupply] = creator;
        nftData[totalSupply].royalty = royalty;
        emit NFTCreated(
            nftName,
            nftSymbol,
            totalSupply,
            totalNFTSupply,
            creator,
            royalty
        );
        totalSupply = totalSupply.add(1);
    }

    /*
        mintFor
        
        Default mint function that requires the edition as a
        parameter. Only one address is allowed to call
        this function. Checks to see if the edition can or has
        been minted.
    */
    function mintFor(
        address to,
        uint256 id,
        uint256 amount,
        uint256 edition,
        bytes memory /*data*/
    ) public virtual onlyMinter {
        require(nftData[id].TOTAL_SUPPLY != 0, "NFT doesn't exist");
        require(nftData[id].TOTAL_SUPPLY > edition, "Edition not availble");
        require(nftData[id].editionsMinted[edition] == false, "Edition minted");
        nftData[id].editionsMinted[edition] = true;
        nftData[id].editionOwnerships[edition] = to;
        nftData[id].balanceOf[to] = nftData[id].balanceOf[to].add(1);
        emit TransferSingleEdition(msgSender(), address(0), to, id, edition, 1);
        ERC1155._mint(to, id, amount, abi.encode(edition));
    }

    /*
        mintEditionBatch

        Default mintBatch function that requires the edition as
        a parameter. Only one address is allowed to call this
        function. Calls mintFor function for the entire batch.
    */
    function mintEditionBatch(
        address to,
        uint256[] calldata ids,
        uint256[] calldata amounts,
        uint256[] calldata editions,
        bytes memory /*data*/
    ) public onlyMinter {
        for (uint256 i = 0; i < ids.length; i++) {
            mintFor(to, ids[i], amounts[i], editions[i], abi.encode(i));
        }
    }

    /*
        mint

        DISABLED. We only allowing minting through mintFor and
        mintEditionBatch.
    */
    function mint(
        address, /*to*/
        uint256, /*id*/
        uint256, /*amount*/
        bytes memory /*data*/
    ) public pure override {
        require(false, "Use 5 arg mintFor");
    }

    /*
        mintBatch

        DISABLED. We only allowing minting through mintFor and
        mintEditionBatch.
    */
    function mintBatch(
        address, /*to*/
        uint256[] calldata, /*ids*/
        uint256[] calldata, /*amounts*/
        bytes memory /*data*/
    ) public pure override {
        require(false, "Use 5 arg mintEditionBatch");
    }

    function msgValue() internal returns (uint256) {
        return msg.value;
    }

    /*
        safeEditionTransferFrom

        The prefered transfer method. If a user owns
        more than one edition, they can specify which
        edition they would like to send. Even if the user
        owns one edition, this transfer method will use
        less gas than safeTransferFrom()
    */
    function safeEditionTransferFrom(
        address sender,
        address receiver,
        uint256 id,
        uint256 amount,
        uint256 edition,
        bytes calldata data
    ) public hasBalance(id, sender) {
        require(nftData[id].editionOwnerships[edition] == sender, "Not Owner");
        emit TransferSingleEdition(
            msgSender(),
            sender,
            receiver,
            id,
            edition,
            msgValue()
        );
        nftData[id].editionOwnerships[edition] = receiver;
        nftData[id].balanceOf[sender] = nftData[id].balanceOf[sender].sub(1);
        nftData[id].balanceOf[receiver] = nftData[id].balanceOf[receiver].add(
            1
        );
        ERC1155.safeTransferFrom(sender, receiver, id, amount, data);
    }

    /*
        safeTransferFrom

        Override existing transfer function to add functionality of
        preserving edition numbers. If multiple editions are owned, the
        highest edition will be sent
    */
    function safeTransferFrom(
        address sender,
        address receiver,
        uint256 id,
        uint256 amount,
        bytes calldata data
    ) public override {
        uint256 edition = highestEdition(sender, id);
        safeEditionTransferFrom(sender, receiver, id, amount, edition, data);
    }

    /*
        safeBatchEditionTransferFrom

        The prefered batchTransfer method. If a user owns
        more than one edition, they can specify which
        edition they would like to send. Even if the user
        owns one edition, this transfer method will use
        less gas than safeBatchTransferFrom()
    */
    function safeBatchEditionTransferFrom(
        address sender,
        address receiver,
        uint256[] calldata ids,
        uint256[] calldata amounts,
        uint256[] calldata editions,
        bytes calldata data
    ) public {
        require(
            ids.length == amounts.length,
            "ERC1155: ids and amounts length mismatch"
        );
        for (uint256 i = 0; i < ids.length; i++) {
            this.safeEditionTransferFrom(
                sender,
                receiver,
                ids[i],
                amounts[i],
                editions[i],
                data
            );
        }
    }

    /*
        safeBatchTransferFrom

        Override existing batchTransfer function to add functionality of
        preserving edition numbers. If multiple editions are owned, the
        highest edition will be sent
    */
    function safeBatchTransferFrom(
        address sender,
        address receiver,
        uint256[] calldata ids,
        uint256[] calldata amounts,
        bytes calldata data
    ) public override {
        require(
            ids.length == amounts.length,
            "ERC1155: ids and amounts length mismatch"
        );
        for (uint256 i = 0; i < ids.length; i++) {
            ArtisLifeNFT.safeTransferFrom(
                sender,
                receiver,
                ids[i],
                amounts[i],
                data
            );
        }
    }

    function burnEdition(
        address account,
        uint256 id,
        uint256 edition
    ) public {
        address operator = _msgSender();
        require(
            account == _msgSender() || isApprovedForAll(account, _msgSender()),
            "ERC1155: caller is not owner nor approved"
        );
        require(nftData[id].editionOwnerships[edition] == account, "Not owner");
        nftData[id].editionOwnerships[edition] = address(0);
        nftData[id].balanceOf[account] = nftData[id].balanceOf[account].sub(1);
        nftData[id].editionsMinted[edition] = false;
        emit TransferSingleEdition(
            operator,
            account,
            address(0),
            id,
            edition,
            1
        );
        ERC1155._burn(account, id, 1);
    }

    function burn(
        address account,
        uint256 id,
        uint256 value
    ) public override {
        require(value == 1, "ERC1155: Can only burn one at a time.");
        uint256 edition = highestEdition(account, id);
        burnEdition(account, id, edition);
    }

    function burnBatch(
        address account,
        uint256[] memory ids,
        uint256[] memory values
    ) public override {
        require(ids.length == values.length, "ids and values legth must match");
        for (uint256 i = 0; i < ids.length; i++) {
            burn(account, ids[i], values[i]);
        }
    }

    /*
        pause

        DISABLED. Reason: reduce contract size
    */
    function pause() public pure override {
        require(false);
    }

    /*
        unpause

        DISABLED. Reason: reduce contract size
    */
    function unpause() public pure override {
        require(false);
    }

    /*
        onlyOwner

        Function to check if the sender of the transaction is
        the owner of this contract.
    */
    modifier onlyOwner() {
        require(hasRole(OWNER_ROLE, msgSender()), "Sender not Govern");
        _;
    }

    /*
        onlyMinter

        Function to check if the sender of the transaction has
        minting priviledges.
    */
    modifier onlyMinter() {
        require(hasRole(MINTER_ROLE, msgSender()), "Sender not minter");
        _;
    }

    /*
        giveRole

        Admin function to give the farming contract minting
        rights to all NFTs. If contract is updated, admin will
        use this function to update accordingly.
    */
    function giveRole(address account) public onlyOwner {
        grantRole(MINTER_ROLE, account);
    }

    /*
        hasBalance

        Function to check if the requested account address has an
        NFT balance of the provided NFT id.
    */
    modifier hasBalance(uint256 id, address sender) {
        require(nftData[id].balanceOf[sender] > 0, "No NFTs owned.");
        _;
    }

    /**
     * Override isApprovedForAll to whitelist user's OpenSea proxy accounts to enable gas-free listings.
     */
    function isApprovedForAll(address _owner, address _operator)
        public
        view
        override
        returns (bool isOperator)
    {
        // Whitelist Marketplace proxy contract for easy trading.\
        if (proxyRegistryAddress[_operator]) {
            return true;
        }

        return ERC1155.isApprovedForAll(_owner, _operator);
    }

    function setProxy(address newAddress, bool newValue) public onlyOwner {
        proxyRegistryAddress[newAddress] = newValue;
    }

    /**
     * This is used instead of msg.sender as transactions won't be sent by the original token owner, but by OpenSea.
     */
    function _msgSender()
        internal
        view
        override(ERC2771Context, Context)
        returns (address sender)
    {
        return ContextMixin.msgSender();
    }
    /**
     * This is used instead of msg.data as transactions won't be sent by the original token owner, but by OpenSea.
     */

    function _msgData()
        internal
        view
        override(ERC2771Context, Context)
        returns (bytes calldata)
    {
        return super._msgData();
    }
}


// File contracts/ARTISTokenBridge_ETH.sol

pragma solidity ^0.8.0;
contract ARTISTokenBridgeMATIC is IERC777Recipient {
    using SafeMath for uint256;
    ERC777 public artisToken;
    ERC20 public feeToken;
    uint256 public TotalValueLocked;
    uint256 public feesCollected;
    uint256 public transactionFee;
    address public ADMIN_ROLE;
    IERC1820Registry private _erc1820 =
        IERC1820Registry(0x1820a4B7618BdE71Dce8cdc73aAB6C95905faD24);
    bytes32 private constant TOKENS_RECIPIENT_INTERFACE_HASH =
        keccak256("ERC777TokensRecipient");
    struct transaction {
        bool isValue;
        uint256 id;
        uint256 amount;
        address user;
        uint256 timestamp;
        uint256 transactionFee;
        bool to_eth;
        bool has_redeemed;
    }

    //store all nft addresses with their added date
    mapping(uint256 => transaction) public depositTxs;
    mapping(uint256 => transaction) public withdrawTxs;
    uint256 public totalTxIDs;

    event BridgeInitiated(
        address user,
        uint256 transactionFee,
        uint256 amount,
        uint256 txID
    );


    event BridgeCompleted(
        address user,
        uint256 transactionFee,
        uint256 amount,
        uint256 txID
    );
    constructor() payable {
        artisToken = ERC777(0xFF1e159e7C67C52cb37B744e5F2e66FeAEfea57E);
        feeToken = ERC20(0x326C977E6efc84E512bB9C30f76E30c160eD06FB); /* WETH Token Address */
        ADMIN_ROLE = msg.sender;
        _erc1820.setInterfaceImplementer(
            address(this),
            TOKENS_RECIPIENT_INTERFACE_HASH,
            address(this)
        );
    }

    /*
        onlyOwner

        Checks if sender is owner of contract
    */
    modifier onlyOwner() {
        require(ADMIN_ROLE == msg.sender, "Caller not owner.");
        _;
    }

    /*
        transactionNotExist

        Checks if transaciton does not exist for given ID
    */
    modifier transactionNotExist(uint256 txID) {
        require(!depositTxs[txID].isValue, "Pool exists already!");
        _;
    }

    /*
        transactionExists

        Checks if transaciton exists for given ID
    */
    modifier transactionExist(uint256 txID) {
        require(depositTxs[txID].isValue, "Pool does not exist!");
        _;
    }

    /*
        nftStakePrice

        Checks the staking price for given txID
    */
    function txFee(uint256 txID)
        public
        view
        transactionExist(txID)
        returns (uint256)
    {
        return depositTxs[txID].transactionFee;
    }

    /*
        tokenReceived

        Begin transaction for given ID. Deposits token into
        contract and logs the time. Tokens will be released to
        same address on secondary chain
    */
    function tokensReceived(
        address operator,
        address from,
        address to,
        uint256 amount,
        bytes calldata txID,
        bytes calldata operatorData
    ) external override {}

    function depositTokens(uint256 amount, uint256 tFee) external {
        uint256 txID = totalTxIDs;
        require(
            tFee == transactionFee,
            "Transaction Fee sent needs to equal fee exactly."
        );
        require(
            transactionFee == depositTxs[txID].transactionFee,
            "Transaction Fee sent needs to equal fee exactly."
        );
        artisToken.transferFrom(msg.sender, address(this), amount);
        feeToken.transferFrom(msg.sender, address(this), transactionFee);
        depositTxs[txID].isValue = true;
        depositTxs[txID].transactionFee = tFee;
        depositTxs[txID].amount = amount;
        depositTxs[txID].user = msg.sender;
        depositTxs[txID].timestamp = block.timestamp;
        depositTxs[txID].to_eth = true;
        depositTxs[txID].has_redeemed = false;
        totalTxIDs = totalTxIDs.add(1);
        emit BridgeInitiated(msg.sender, transactionFee, amount, txID);
    }

    /**
        enableBridge

        Admin function to bridge data from one blockchain to another.
    */

    function completeBridge(
        uint256 txID,
        uint256 tFee,
        uint256 amount,
        address user
    ) public onlyOwner {
        require(withdrawTxs[txID].isValue == false, "Tx already created.");
        withdrawTxs[txID].isValue = true;
        withdrawTxs[txID].transactionFee = tFee;
        withdrawTxs[txID].amount = amount;
        withdrawTxs[txID].user = user;
        withdrawTxs[txID].timestamp = block.timestamp;
        withdrawTxs[txID].to_eth = false;
        withdrawTxs[txID].has_redeemed = true;
        artisToken.send(
            depositTxs[txID].user,
            depositTxs[txID].amount,
            abi.encodePacked("Tokens Bridged.")
        );
        emit BridgeCompleted(user, transactionFee, amount, txID);
    }

    /*
        collectFees

        Admin function used to withdraw the fees that have
        been collected on the network thus far. 
    */
    function collectFees() public onlyOwner {
        artisToken.send(
            msg.sender,
            feesCollected,
            abi.encodePacked("Collected Fees.")
        );
        TotalValueLocked = TotalValueLocked.sub(feesCollected);
        feesCollected = 0;
    }

    /*
        setERC777Address

        Update the token address used for staking.
    */
    function setERC777Address(address artisAddress) public onlyOwner {
        artisToken = ERC777(artisAddress);
    }

        /*
        setFeeAddress

        Update the token address used for staking.
    */
    function setFeeAddress(address feeTokenAddress) public onlyOwner {
        feeToken = ERC20(feeTokenAddress);
    }

    /*
        setFee

        Update the network fee of the given txID.
    */
    function setFee(uint256 _fee) public onlyOwner {
        transactionFee = _fee;
    }

    function emergencyWithdrawERC20(address a) external onlyOwner {
        ERC20 token = ERC20(a);
        token.transfer(msg.sender, artisToken.balanceOf(address(this)));
        TotalValueLocked = artisToken.balanceOf(address(this));
    }

    function emergencyWithdrawERC777(address a) external onlyOwner {
        ERC777 token = ERC777(a);
        token.send(
            msg.sender,
            artisToken.balanceOf(address(this)),
            "Emergency Withdraw"
        );
        TotalValueLocked = artisToken.balanceOf(address(this));
    }

    function emergencyNFTWithdraw(address a, uint256 id) external onlyOwner {
        ERC1155 i = ERC1155(a);
        i.safeTransferFrom(
            address(this),
            msg.sender,
            id,
            i.balanceOf(address(this), id),
            "Emergency Withdraw"
        );
    }

    function emergencyNFT721Withdraw(address a, uint256 id) external onlyOwner {
        ERC721 i = ERC721(a);
        i.transferFrom(address(this), msg.sender, id);
    }
}