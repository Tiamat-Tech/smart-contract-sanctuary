pragma solidity 0.8.6;

contract IndividualPriceModel {
    uint256 private _defaultpricePerDay;

    constructor() {
        _defaultpricePerDay = 10 ether;
    }

    function individualPrice(uint256 duration, uint256 individualPricePerDay) external view returns (uint256) {
        return individualPricePerDay * duration;
    }

    function price(uint256 duration) external view returns (uint256) {
        return _defaultpricePerDay * duration;
    }

    function setMinimumPricePerDay(uint256 price_) external {
        _defaultpricePerDay = price_;
    }

    function isIndividualPricing() external pure returns (bool) {
        return true;
    }

    function defaultPricePerDay() external view returns (uint256) {
        return _defaultpricePerDay;
    }
}