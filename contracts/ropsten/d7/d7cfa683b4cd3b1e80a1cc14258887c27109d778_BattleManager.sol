// File: BattleManager.sol


pragma solidity ^0.8.0;








contract BattleManager is Context,Ownable{
    using SafeMath for uint256;
    using Counters for Counters.Counter;
    using EnumerableSet for EnumerableSet.UintSet;
    Counters.Counter private _idTracker;
    
    struct Player {
        address id;
        uint256 id_token;
        bool ready;
    }

    struct Battle {
        uint256 id;
        address host;
        Player p1;
        Player p2;
        uint256 price;
        bool started;
        uint256 bornTime;
    }
    
    event Create(uint256 indexed id,address sender);
    event Join(uint256 indexed id,address sender);
    event Leave(uint256 indexed id,address sender);
    event Start(uint256 indexed id,address sender);
    event End(uint256 indexed id,address sender,address winner,uint256 price);
    
    mapping(uint256 => Battle) internal battles;
    mapping(address => uint256) private address_battles;
    EnumerableSet.UintSet private index_battles;

    modifier onlyBattle() {
        require(address_battles[_msgSender()] >= 0, "onlyBattle");
        _;
    }
    
    modifier onlyHostBattle(uint256 _id) {
        require(battles[_id].host == _msgSender(), "onlyHostBattle");
        _;
    }
    
    NFTInterface nft_address;
    address private admin;
    uint256 public fee = 9;
    
    constructor(NFTInterface _address){
        nft_address = _address;
        admin = address(this);
    }
    
    function setNFT(NFTInterface _address) public onlyOwner{
        nft_address = _address;
    }
    
    function setAdmin(address _address) public onlyOwner{
        admin = _address;
    }
    
    function setFee(uint256 _fee) public onlyOwner{
        fee = _fee;
    }
    
    function create(uint256 _tokenId,uint256 _price) public returns (bool) {
        address sender = _msgSender();
        require(address_battles[sender] >= 0,"created");
        require(nft_address.ownerOf(_tokenId) == sender,"ownerOf");
        require(nft_address.getErc20().allowance(sender,address(this)) >= _price,"allowance");
        uint256 nextId = _idTracker.current();
        battles[nextId] = Battle({
            id : nextId,
            host : sender,
            p1 : Player({
                id : sender,
                id_token : _tokenId,
                ready : true
            }),
            p2 : Player({
                id : address(0),
                id_token : 0,
                ready : false
            }),
            started : false,
            price : _price,
            bornTime : block.timestamp
        });
        address_battles[sender] = nextId;
        index_battles.add(nextId);
        emit Create(nextId,sender);
        return true;
    }
    
    function join(uint256 _id,uint256 _tokenId) public returns (bool) {
        address sender = _msgSender();
        require(nft_address.ownerOf(_tokenId) == sender,"ownerOf");
        Battle storage battle = battles[_id];
        require(nft_address.getErc20().allowance(sender,address(this)) >= battle.price,"allowance");
        battle.p2.id = sender;
        battle.p2.ready = true;
        battle.p2.id_token = _tokenId;
        address_battles[sender] = _id;
        emit Join(_id,sender);
        return true;
    }
    
    function leave(uint256 _id) public onlyBattle returns (bool) {
        address sender = _msgSender();
        Battle storage battle = battles[_id];
        if(battle.p2.id == sender){
            battle.p2 = Player({
                id : address(0),
                id_token : 0,
                ready : false
            });
            delete address_battles[sender];
        }
        if(battle.host == sender){
            clear(_id);
        }
        emit Leave(_id,sender);
        return true;
    }
    
    function start(uint256 _id) public onlyBattle onlyHostBattle(_id) returns (bool) {
        address sender = _msgSender();
        Battle storage battle = battles[_id];
        if(!battle.started && battle.p1.ready && battle.p2.ready){
            emit Start(_id,sender);
            battle.started = true;
        }
        return true;
    }
    
    function end(uint256 _id,uint256 _winner) public onlyOwner returns (bool) {
        address sender = _msgSender();
        Battle storage battle = battles[_id];
        require(!battle.started,"not started");
        Player memory winner = battle.p1;
        Player memory loser = battle.p2;
        if(_winner == 2){
            winner = battle.p2;
            loser = battle.p1;
        }
        uint256 price = battle.price;
        require(nft_address.getErc20().transferFrom(loser.id,winner.id,price),"Foser pay to winner faild");
        uint256 feeBattle = price.div(fee);
        require(nft_address.getErc20().transferFrom(winner.id,admin,feeBattle),"Winner pay to admin faild");
        clear(_id);
        emit End(_id,sender,winner.id,price);
        return true;
    }
    
    function clear(
        uint256 _id
    ) internal returns(bool) {
        address sender = _msgSender();
        delete battles[_id];
        delete address_battles[sender];
        index_battles.remove(_id);
        return true;
    }
    
    function size() public view returns (uint256) {
        return index_battles.length();
    }

    function getBattle(uint256 _id) public view returns (Battle memory) {
        //if (index_battles.contains(_id)) 
        return battles[_id];
    }
}